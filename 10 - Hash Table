
#include <stdio.h>
#include<stdlib.h>

int key[20], m, n;
int *ht, index;
int count = 0;

void insert(int key)
{
    index = key % m;
    while (ht[index] != -1)
    {
        index = (index + 1) % m;
    }
    ht[index] = key;
    count++;
}

void display()
{
    int i;
    if (count == 0)
    {
        printf("The hash table is empty\n");
        return;
    }

    printf("\nThe elements of the hash table are: ");

    for (int i = 0; i < m; i++)
    {
        printf("\nT[%d] -> %d", i, ht[i]);
    }
}

void main()
{
    int i;
    printf("\nEnter the number of employees: ");
    scanf("%d", &n);

    printf("\nEnter the number of memory locations: ");
    scanf("%d", &m);

    ht = (int *)malloc(m * sizeof(int));

    for (i = 0; i < m; i++)
    {
        ht[i] = -1;
    }

    printf("\nEnter the four digit employee key:\n");
    for (i = 0; i < n; i++)
    {
        scanf("%d", &key[i]);
    }
    for (i = 0; i < n; i++)
    {
        if (count == m)
        {
            printf("The hash table is full\n");
            break;
        }
        insert(key[i]);
    }
    display();
}
